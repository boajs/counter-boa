{
  "name": "counter-boa",
  "description": "A b-o-a example.",
  "version": "0.0.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "ava": {
    "files": [
      ".tmp/test"
    ],
    "source": [
      "!**/*.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/counter-boa/issues"
  },
  "dependencies": {
    "b-o-a": "0.8.0"
  },
  "devDependencies": {
    "ava": "0.13.0",
    "less": "^2.6.1",
    "parallelshell": "2.0.0",
    "proxyquire": "1.7.4",
    "sinon": "1.17.3",
    "source-map-loader": "^0.1.5",
    "typescript": "1.8.7",
    "typings": "0.7.8",
    "watch": "^0.17.1",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "homepage": "https://github.com/bouzuya/counter-boa#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bouzuya/counter-boa.git"
  },
  "scripts": {
    "build": "tsc && webpack && npm run build-less && npm run build-html",
    "build-less": "lessc src/styles/index.less dist/styles/index.css",
    "build-html": "([ -d dist ] || mkdir -p dist) && cp index.html dist",
    "clean": "rm -rf .tmp/ dist/",
    "prepublish": "typings install && npm run clean && npm run build",
    "test": "ava",
    "watch": "npm run clean && npm run build-html && parallelshell 'tsc --watch' 'ava --watch' 'npm run watch-less' 'webpack-dev-server --config webpack.dev.config.js'",
    "watch-less": "watch --ignoreUnreadable --wait 5 'npm run build-less' src/styles/"
  }
}
